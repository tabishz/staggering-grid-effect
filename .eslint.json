{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:ava/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {
    "ava/no-ignored-test-files": "off",
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1
      }
    ]
  }
}